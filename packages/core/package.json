{
  "name": "@composifx/core",
  "version": "0.1.0",
  "description": "Core composition engine for ComposiFX",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "compositing",
    "animation",
    "motion-graphics"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0"
  }
}
